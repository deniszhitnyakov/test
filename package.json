    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "eslint": "^6.8.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "flow-bin": "^0.125.1",
    "sass": "^1.19.0",
