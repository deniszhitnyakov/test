  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "plugins": [
      "arca",
      "html"
    ],
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "rules": {
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 1,
          "maxEOF": 1
        }
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "indent": [
        2
      ],
      "arca/import-align": 2,
      "arca/import-ordering": 2,
      "arca/newline-after-import-section": 2
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}