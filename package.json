    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "roboto-fontface": "*",
    "v-clipboard": "^2.2.2",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-flag-icon": "^1.0.6",
